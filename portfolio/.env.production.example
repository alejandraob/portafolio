# IMPORTANTE: Copia estos valores a las Environment Variables en Render
# No subas este archivo con valores reales a GitHub

# Application
APP_NAME="Portfolio María Alejandra Ojeda"
APP_ENV=production
APP_KEY=base64:TU_CLAVE_AQUI_GENERA_UNA_NUEVA
APP_DEBUG=false
APP_URL=https://tu-app.onrender.com

# Database - Render te proporciona estos valores
DB_CONNECTION=mysql
DB_HOST=tu-db-host.render.com
DB_PORT=3306
DB_DATABASE=portfolio
DB_USERNAME=portfolio_user
DB_PASSWORD=TU_PASSWORD_DE_RENDER

# Cache & Session
LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=error

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

# Redis (opcional - Render lo proporciona)
MEMCACHED_HOST=127.0.0.1
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Mail - Gmail
MAIL_MAILER=smtp
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USERNAME=alejandraob21@gmail.com
MAIL_PASSWORD="hgpu veoj pevu cnss"
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS="alejandraob21@gmail.com"
MAIL_FROM_NAME="Portfolio - María Alejandra Ojeda"

# AWS (no necesario por ahora)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# Pusher (no necesario por ahora)
PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_HOST=
PUSHER_PORT=443
PUSHER_SCHEME=https
